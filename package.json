{
	"name": "b",
	"version": "0.0.1",
	"sideEffects": false,
	"type": "module",
	"types": "./dist/src/index.d.ts",
	"typesVersions": {
	  "*": {
		"*": [
		  "*",
		  "dist/*",
		  "dist/src/*",
		  "dist/src/*/index"
		],
		"src/*": [
		  "*",
		  "dist/*",
		  "dist/src/*",
		  "dist/src/*/index"
		]
	  }
	},
	"files": [
	  "src",
	  "dist",
	  "!dist/test",
	  "!**/*.tsbuildinfo"
	],
	"exports": {
	  ".": {
		"types": "./dist/src/index.d.ts",
		"import": "./dist/src/index.js"
	  }
	},
	"eslintConfig": {
		"extends": "ipfs",
		"parserOptions": {
		  "project": true,
		  "sourceType": "module"
		},
		"ignorePatterns": [
		  "!.aegir.js",
		  "test/ts-use",
		  "*.d.ts"
		]
	  },
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
	    "clean": "aegir clean",
		"build": "aegir build",
		"test": "aegir test",
		"test:node": "aegir test -t node -f \"./dist/test/**/*.{node,spec}.js\" --cov"
	},
	"license": "MIT",
	"dependencies": {
		"mocha": "^10.4.0"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.6",
		"ts-node": "^10.9.2",
		"typescript": "^5.4.3",
		"chai": "^5.1.0",
		"@types/chai": "^4.3.14"

	}
}